<h1><%= @doctor.name %></h1>

<h2>Department | <%= @doctor.department %></h2>

<h2>Appointments</h2>
<ul>
  <% @doctor.appointments.each do |appointment| %>
    <li><%= appointment.appointment_datetime.strftime('%B %d, %Y at %H:%M') %>
     | <%= link_to appointment.patient.name, appointment.patient %></li>
  <% end %>
</ul>
