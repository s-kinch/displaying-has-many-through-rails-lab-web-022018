<h1><%= @patient.name %></h1>
<p>Age | <%= @patient.age %></p>
<p>Appointments</p>
<ul>
  <% @patient.appointments.each do |appointment| %>
    <li><%= appointment.appointment_datetime.strftime('%B %d, %Y at %H:%M') %>
     | <%= link_to appointment.doctor.name, appointment.doctor %></li>
  <% end %>
</ul>
